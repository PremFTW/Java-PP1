package firstJavaPrograms;

import java.util.Random;

public class FinalReview1 {

	public static void main(String[] args) {

		int[] x = new int[100];
		int num;
		
		Random r = new Random();
		
		for(int i = 0; i < 100; i++)
		{
			num = r.nextInt(9);
			x[i] = num;
		}
		
		displayArray(x);

	}
	
	public void displayArray(int[] array)
	{
		for(int i = 0; i < 100; i++)
		{
			System.out.println(array[i]);
		}
	}

}
