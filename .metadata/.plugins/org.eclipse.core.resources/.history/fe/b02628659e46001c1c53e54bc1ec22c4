package homework;

// Declaring these to read, write, and use files
import java.io.File;
import java.io.PrintWriter;
import java.util.Scanner;

// Declaring this to use random numbers
import java.util.Random;

public class HW10 {

	public static void main(String[] args) {
		
		// Part A Code
		// Creating a file
		File file = new File("hw10.txt");
		
		// Checking if the file already exists
		if(file.exists())
		{
			System.out.println("File already exists.");
			return;
		}
		
		// Declared this to store the random number
		int num;
		
		// Creating a new random object
		Random r = new Random();
		
		PrintWriter partA = new PrintWriter(file);
		
		for(int i = 0; i < 10; i++)
		{
			num = r.nextInt(99);
		}
		
		partA.close();
	}

}
